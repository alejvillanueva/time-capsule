{
	"name": "memora",
	"version": "1.0.0",
	"main": "index.js",
	"type": "module",
	"scripts": {
		"server:dev": "tsx --watch --env-file=.env ./server/index.ts",
		"dev": "concurrently \"tsx --watch --env-file=.env ./server/index.ts\" \"cd ./client && npm run dev\"",
		"start": "node ./dist/index.js",
		"start-dev": "node --env-file=.env ./dist/index.js",
		"type-check": "tsc --noEmit",
		"build": "npm install && cd client && npm install && cd .. && concurrently \"tsc\" \"cd ./client && npm run build\"",
		"lint:check": "eslint .",
		"lint": "eslint --fix .",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"dependencies": {
		"@cloudinary/react": "^1.14.3",
		"@cloudinary/url-gen": "^1.21.0",
		"axios": "^1.9.0",
		"concurrently": "^9.1.2",
		"cors": "^2.8.5",
		"express": "^4.21.2",
		"pg": "^8.14.1"
	},
	"devDependencies": {
		"@eslint/js": "^9.23.0",
		"@types/cors": "^2.8.17",
		"@types/express": "^5.0.1",
		"@types/node": "^22.13.11",
		"@types/pg": "^8.11.11",
		"@typescript-eslint/parser": "^8.27.0",
		"eslint": "^9.23.0",
		"prettier": "^3.5.3",
		"tsx": "^4.19.3",
		"typescript": "^5.8.2",
		"typescript-eslint": "^8.27.0"
	}
}
